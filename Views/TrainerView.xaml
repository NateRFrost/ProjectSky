                            <hc:NumericUpDown Grid.Row="1" Grid.Column="6" Margin="5" Value="{Binding SelectedTrainer.Data.poke6.effortValue.spAtk}" Minimum="0" Maximum="255"/>
                            <hc:NumericUpDown Grid.Row="2" Grid.Column="6" Margin="5" Value="{Binding SelectedTrainer.Data.poke6.effortValue.spDef}" Minimum="0" Maximum="255"/>
                            <hc:NumericUpDown Grid.Row="3" Grid.Column="6" Margin="5" Value="{Binding SelectedTrainer.Data.poke6.effortValue.agi}" Minimum="0" Maximum="255"/>

                        </Grid>

                    </Border>

                    <Border Grid.Row="0" Grid.Column="2" Background="#202020" BorderBrush="{x:Null}" Margin="10" CornerRadius="5">
                        <Grid>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="210"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="160"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <hc:ImageBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Source="{Binding SelectedTrainer.Data.poke6.sprite}" Height="125" Width="125"/>
                            <hc:ComboBox Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0 0 0 10" Height="25" Width="175" ItemsSource="{Binding PkDevID.values}" DisplayMemberPath="name" SelectedValuePath="devName" SelectedValue="{Binding SelectedTrainer.Data.poke6.devId}"/>
                            <hc:NumericUpDown Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0 0 0 10" Height="25" Width="60" Value="{Binding SelectedTrainer.Data.poke6.formId}"/>
                        </Grid>
                    </Border>

                    <Border Grid.Row="1" Grid.Column="0" Background="#202020" BorderBrush="{x:Null}" Margin="10" CornerRadius="5">

                        <Grid>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="200"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="25"/>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Moves" FontFamily="{StaticResource Gill}" FontSize="30"/>

                            <ComboBox ItemsSource="{Binding Moves.moves}" DisplayMemberPath="name" SelectedValuePath="devName" SelectedValue="{Binding SelectedTrainer.Data.poke6.waza1.wazaId}" Grid.Row="1" Grid.Column="0" Margin="2"/>
                            <ComboBox ItemsSource="{Binding Moves.moves}" DisplayMemberPath="name" SelectedValuePath="devName" SelectedValue="{Binding SelectedTrainer.Data.poke6.waza2.wazaId}" Grid.Row="2" Grid.Column="0" Margin="2"/>
                            <ComboBox ItemsSource="{Binding Moves.moves}" DisplayMemberPath="name" SelectedValuePath="devName" SelectedValue="{Binding SelectedTrainer.Data.poke6.waza3.wazaId}" Grid.Row="3" Grid.Column="0" Margin="2"/>
                            <ComboBox ItemsSource="{Binding Moves.moves}" DisplayMemberPath="name" SelectedValuePath="devName" SelectedValue="{Binding SelectedTrainer.Data.poke6.waza4.wazaId}" Grid.Row="4" Grid.Column="0" Margin="2"/>
                            <hc:NumericUpDown Grid.Row="1" Grid.Column="1" Value="{Binding SelectedTrainer.Data.poke6.waza1.pointUp}" Minimum="0" Maximum="3" Margin="2"/>
                            <hc:NumericUpDown Grid.Row="2" Grid.Column="1" Value="{Binding SelectedTrainer.Data.poke6.waza2.pointUp}" Minimum="0" Maximum="3" Margin="2"/>
                            <hc:NumericUpDown Grid.Row="3" Grid.Column="1" Value="{Binding SelectedTrainer.Data.poke6.waza3.pointUp}" Minimum="0" Maximum="3" Margin="2"/>
                            <hc:NumericUpDown Grid.Row="4" Grid.Column="1" Value="{Binding SelectedTrainer.Data.poke6.waza4.pointUp}" Minimum="0" Maximum="3" Margin="2"/>

                            <CheckBox Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" Content="Random?" HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding ToggleRandomMovesCommand}">
                                <CheckBox.CommandParameter>
                                    <MultiBinding Converter="{StaticResource CommandMultiValueConv}">
                                        <Binding RelativeSource="{RelativeSource Self}" Path="."/>
                                        <Binding>
                                            <Binding.Source>
                                                <sys:String>6</sys:String>
                                            </Binding.Source>
                                        </Binding>
                                    </MultiBinding>
                                </CheckBox.CommandParameter>
                                <CheckBox.IsChecked>
                                    <MultiBinding Converter="{StaticResource MoveStringToBool}">
                                        <Binding Path="SelectedTrainer.Data.poke6.wazaType"/>
                                        <Binding RelativeSource="{RelativeSource Self}" Path="."/>
                                    </MultiBinding>
                                </CheckBox.IsChecked>
                            </CheckBox>
                        </Grid>

                    </Border>

                    <Border Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Background="#202020" BorderBrush="{x:Null}" Margin="10" CornerRadius="5">

                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Miscellaneous Options" FontSize="30" FontFamily="{StaticResource Gill}"/>
                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Ball" FontSize="15" FontFamily="{StaticResource Gill}"/>
                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Tera Type" FontSize="15" FontFamily="{StaticResource Gill}"/>
                            <TextBlock Grid.Row="2" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Item" FontSize="15" FontFamily="{StaticResource Gill}"/>
                            <TextBlock Grid.Row="4" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Ability" FontSize="15" FontFamily="{StaticResource Gill}"/>
                            <TextBlock Grid.Row="2" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Level" FontSize="15" FontFamily="{StaticResource Gill}"/>
                            <TextBlock Grid.Row="4" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Shiny?" FontSize="15" FontFamily="{StaticResource Gill}"/>

                            <ComboBox Grid.Row="2" Grid.Column="1"  ItemsSource="{Binding BallItemID}" DisplayMemberPath="Key" SelectedValuePath="Value" SelectedValue="{Binding SelectedTrainer.Data.poke6.ballId}"/>
                            <ComboBox Grid.Row="4" Grid.Column="1" ItemsSource="{Binding TeraTypes}" DisplayMemberPath="Key" SelectedValuePath="Value" SelectedValue="{Binding SelectedTrainer.Data.poke6.gemType}"/>
                            <ComboBox Grid.Row="2" Grid.Column="3" ItemsSource="{Binding ItmDevID.items}" DisplayMemberPath="name" SelectedValuePath="devName" SelectedValue="{Binding SelectedTrainer.Data.poke6.item}"/>
                            <ComboBox Grid.Row="4" Grid.Column="3" ItemsSource="{Binding AbilityTr}" DisplayMemberPath="Key" SelectedValuePath="Value" SelectedValue="{Binding SelectedTrainer.Data.poke6.tokusei}"/>

                            <hc:NumericUpDown Grid.Row="2" Grid.Column="5" Value="{Binding SelectedTrainer.Data.poke6.level}" Minimum="1" Maximum="100"/>

                            <CheckBox Grid.Row="4" Grid.Column="5" HorizontalAlignment="Left" VerticalAlignment="Center" Command="{Binding ToggleShinyCommand}" IsChecked="{Binding SelectedTrainer.Data.poke6.rareType, Converter={StaticResource RareStringToBool}}">
                                <CheckBox.CommandParameter>
                                    <MultiBinding Converter="{StaticResource CommandMultiValueConv}">
                                        <Binding RelativeSource="{RelativeSource Self}" Path="."/>
                                        <Binding>
                                            <Binding.Source>
                                                <sys:String>6</sys:String>
                                            </Binding.Source>
                                        </Binding>
                                    </MultiBinding>
                                </CheckBox.CommandParameter>
                            </CheckBox>

                        </Grid>

                    </Border>

                </Grid>
            </hc:TabItem>

            <hc:TabItem Header="Trainer Options" Background="#252525">

                <Border Background="#202020" BorderBrush="{x:Null}" Margin="10" CornerRadius="5">

                    <Grid>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="100"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Trainer Options" FontFamily="{StaticResource Gill}" FontSize="60"/>
                        <TextBlock Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="5" Text="Money Rate" FontFamily="{StaticResource Gill}"/>
                        <hc:NumericUpDown Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="5" Width="75" Minimum="0" Maximum="100" Value="{Binding SelectedTrainer.Data.moneyRate}"/>
                        <CheckBox Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Is Strong?" IsChecked="{Binding SelectedTrainer.Data.isStrong}"/>
                        <CheckBox Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Basic Difficulty AI" IsChecked="{Binding SelectedTrainer.Data.aiBasic}"/>
                        <CheckBox Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="High Difficulty AI" IsChecked="{Binding SelectedTrainer.Data.aiHigh}"/>
                        <CheckBox Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Expert Difficulty AI" IsChecked="{Binding SelectedTrainer.Data.aiExpert}"/>
                        <CheckBox Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Doubles AI" IsChecked="{Binding SelectedTrainer.Data.aiDouble}"/>
                        <CheckBox Grid.Row="7" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Raid AI" IsChecked="{Binding SelectedTrainer.Data.aiRaid}"/>
                        <CheckBox Grid.Row="8" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Weak AI" IsChecked="{Binding SelectedTrainer.Data.aiWeak}"/>
                        <CheckBox Grid.Row="9" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Can Use Items?" IsChecked="{Binding SelectedTrainer.Data.aiItem}"/>
                        <CheckBox Grid.Row="10" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Can Change Pokemon?" IsChecked="{Binding SelectedTrainer.Data.aiChange}"/>
                        <CheckBox Grid.Row="11" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Can Terastalize?" IsChecked="{Binding SelectedTrainer.Data.changeGem}"/>

                    </Grid>
                    
                </Border>

            </hc:TabItem>
    
        </hc:TabControl>
    </Grid>
</UserControl>
